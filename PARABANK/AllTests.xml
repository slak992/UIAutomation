<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite" parallel="classes" thread-count="5">
<listeners>
 <listener class-name="com.parabank.ui.base.TestListner"/>
 <listener class-name="com.parabank.ui.base.AnnotationTransformer"/>
</listeners>
  <test  name="Login">
  
    <classes>
      <class name="com.parabank.ui.tests.pos.POS_Login_Test"/>
      <class name="com.parabank.ui.tests.pos.POS_Account_Test"/>
      <class name="com.parabank.ui.tests.pos.POS_loanProcessingTest"/>
      <class name="com.parabank.ui.tests.pos.POS_TransferFunds"/>
      <class name="com.parabank.ui.tests.pos.POS_Transaction"/>
    </classes>
  </test> <!-- Test -->
  <test name="Negative Tests" parallel="methods">
  	<classes>
  		<class name="com.parabank.ui.tests.neg.NEG_loanProcessingTest"/>
  		<class name="com.parabank.ui.tests.neg.NEG_Login_Test"/>
  	</classes>
  </test>
  <test  name="Account Settings">
    <classes>    
      <class name="com.parabank.ui.tests.pos.POS_OpenNewAccount"/> 
    </classes>
   </test>
</suite> <!-- Suite -->
