<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite" parallel="methods" thread-count="5">
	<listeners>
		<listener class-name="com.parabank.ui.base.TestListner" />
	</listeners>
	<test name="Account Settings">
		<parameter name="BalanceTest" value="Checking Balance in the accounts" />
		<classes>

			<class name="com.parabank.ui.tests.pos.POS_OpenNewAccount">
				<methods>
					<exclude name=".*Balance.*" />
				</methods>
			</class>


		</classes>
	</test>
	<test name="Neg Tests">
		<classes>
			<class name="com.parabank.ui.tests.neg.NEG_Login_Test">
				<methods>
					<include name="NEG_registerUserWithFirstNameBlank"></include>
				</methods>
			</class>
		</classes>

	</test> <!-- Test -->
</suite> <!-- Suite -->