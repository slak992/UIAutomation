<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite" parallel="methods" thread-count="10">
	<listeners>
		<listener class-name="com.parabank.ui.base.TestListner" />
	</listeners>
	<test name="Package Level execution Example">
		<groups>
			<run>
				<include name="regression"></include>
			</run>
		</groups>

		<packages>
			<package name="com.parabank.ui.tests.neg"></package>
		</packages>
	</test> <!-- Test -->
</suite> <!-- Suite -->